ll powermod(ll x1, ll n1){
	if(n1 == 0) return 1;
	else if(n1%2==0) return powermod((x1*x1)%M, n1/2);
	else return (x1*powermod((x1*x1)%M, (n1-1)/2))%M;
}
]]></content>
